<div>
	<h1>Lista de Productos</h1>
	{{#if products}}
		{{#each products}}
			<h2>{{this.title}}</h2>
			<p > Descripcion: {{this.description}}</p>
			<p > Codigo de producto: {{this.code}}</p>
			<p > Precio: {{this.price}}</p>
			<p > Stock: {{this.stock}}</p>
			<p > ID: {{this.id}}</p>
			{{#if this.thumbnails}}
				<p > Imagen:  {{this.thumbnails}}</p>
			{{/if}}
		{{/each}}
	{{else}}
		<h3>No hay productos agregados</h3>
	{{/if}}
</div>